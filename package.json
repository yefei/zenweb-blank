    "debug": "^4.3.2",
    "moment": "^2.29.1",
    "mysql-easy-query": "^2.0.0",
    "sql-easy-builder": "^2.0.2",
    "zenorm": "^2.2.0",
    "zenweb": "^2.3.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "tsc-watch": "^4.6.0",
    "typescript": "^4.5.4"
